{
  "compilerOptions": {
    "lib": [
      "ESNext",
      "dom",
    ],
    "module": "commonjs",
    "target": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./",
    "watch": true,
    "declaration": false,
    "noUnusedLocals": false,
    "incremental": false,
    "experimentalDecorators": true
  },
  "include": [
    "./*.ts"
  ],
  "exclude": [
    "evn.ts"
  ]
}